/**app.wxss**/
.indexFixedBar{
    width: 100%;
    height: 54px;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    box-shadow: 0px 0px 10px 0px rgb(212, 212, 212);
    position: fixed;
    bottom: 0;
    left: 0;
    display: flex;
    flex-direction: row;
    background: #fff;
}
.bar-left,.bar-right,.bar-right-1{
    height: 100%;
    width: 33%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 12px;
    color: #bfbfbf;
}
.bar-font-blue{
    color: #1296db;
}
.bar-middle{
    width: 0.5%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    color: #bfbfbf;
}
.bar-img{
    width: 30px;
    height: 30px;
}
.hover-bar{
    background: rgb(252, 252, 252);
}